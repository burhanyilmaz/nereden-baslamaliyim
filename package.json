{
  "name": "nereden-baslamaliyim",
  "version": "1.0.0",
  "description": "Bilgisayar dünyasına giriş yapmak isteyip nereden başlayacağını bilmeyenlere yol göstermek.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "start": "parcel ./public/index.html",
    "build": "rm -rf .cache && rm -rf dist && parcel build ./public/index.html  --public-url ./"
  },
  "keywords": [
    "nereden-başlamalıyım",
    "topluluk",
    "yazılım"
  ],
  "author": "Burhan Yılmaz",
  "license": "ISC",
  "dependencies": {
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "rmwc": "^1.7.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-jest": "^22.4.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-react": "^7.8.2",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.7.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/bootstrapTests.js",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
